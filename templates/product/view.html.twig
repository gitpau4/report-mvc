{% extends 'base.html.twig' %}

{% block title %}View{% endblock %}

{% block body %}
<div class="example-wrapper">
    <h1>Products view</h1>

    <ul>
    {% for product in products %}
        <li>{{ product.getName|e }} ({{ product.getId|e }}) {{ product.getValue|e }}</li>
    {% endfor %}
    </ul>
</div>
{% endblock %}
